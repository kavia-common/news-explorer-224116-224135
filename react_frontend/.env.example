# Environment variables for News Explorer (copy as .env)
REACT_APP_NODE_ENV=development
REACT_APP_PORT=3000

# Newsdata.io API
# Obtain from https://newsdata.io/
REACT_APP_NEWSDATA_API_KEY=your_newsdata_api_key_here
# Optional override for API base (defaults to https://newsdata.io/api/1)
REACT_APP_API_BASE=https://newsdata.io/api/1

# Supabase (optional; if absent, bookmarks fall back to localStorage)
REACT_APP_SUPABASE_URL=https://your-project-id.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your_anon_key_here
